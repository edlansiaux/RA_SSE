  # === PROTOCOLES ASSOCIÉS ===
  associatedProtocols:
    - protocolName: "ABCDE - Trauma grave"
      description: "Évaluation et prise en charge du traumatisé grave"
      steps:
        - "A - Airway: Contrôle des voies aériennes + stabilisation rachis cervical"
        - "B - Breathing: Oxygène, drainage thoracique si nécessaire"
        - "C - Circulation: Accès IV/IO, remplissage, catécholamines"
        - "D - Disability: Score Glasgow, pupilles, glycémie"
        - "E - Exposure: Déshabillage complet, protection thermique"
        - "Réévaluation continue"
        - "Orientation vers trauma center"
      applicableTriageCategories:
        - "ROUGE"
    - protocolName: "Intubation séquence rapide"
      description: "Protocole d'intubation en urgence"
      steps:
        - "Préparation matériel (vérification)"
        - "Préoxygénation 3 minutes"
        - "Induction: hypnotique + curare"
        - "Manœuvre de Sellick"
        - "Laryngoscopie directe"
        - "Intubation + vérification placement"
        - "Fixation + ventilation contrôlée"
        - "Sédation d'entretien"
      applicableTriageCategories:
        - "ROUGE"
    - protocolName: "Choc hémorragique"
      description: "Prise en charge du choc hémorragique traumatique"
      steps:
        - "Contrôle hémorragie (garrot, packing)"
        - "2 voies veineuses gros calibre"
        - "Remplissage NaCl objectif PAS > 80-90"
        - "Acide tranexamique 1g IV si < 3h"
        - "Noradrénaline si remplissage insuffisant"
        - "Hypothermie permissive"
        - "Transport immédiat trauma center"
      applicableTriageCategories:
        - "ROUGE"
  

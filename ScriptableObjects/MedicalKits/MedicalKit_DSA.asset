  # === PROTOCOLES ASSOCIÉS ===
  associatedProtocols:
    - protocolName: "Défibrillation - Arrêt cardiaque"
      description: "Utilisation du DSA en cas d'arrêt cardiaque"
      steps:
        - "Confirmer l'arrêt cardiaque (inconscience + absence de respiration)"
        - "Demander un DSA et appeler le 15"
        - "Commencer la RCP immédiatement"
        - "Dès disponibilité: allumer le DSA"
        - "Dénuder le thorax, sécher si nécessaire, raser si pilosité importante"
        - "Coller les électrodes selon schéma (sous-claviculaire D + apex G)"
        - "Suivre les instructions vocales du DSA"
        - "S'écarter lors de l'analyse et du choc"
        - "Reprendre la RCP immédiatement après le choc"
        - "Continuer jusqu'à relève par équipe médicale ou reprise de vie"
      applicableTriageCategories:
        - "ROUGE"
    - protocolName: "DSA - Cas particuliers"
      description: "Situations particulières d'utilisation du DSA"
      steps:
        - "Victime mouillée: sécher le thorax avant pose des électrodes"
        - "Pacemaker visible: placer électrode à 8cm du boîtier"
        - "Timbre médicamenteux: retirer et nettoyer la zone"
        - "Enfant 1-8 ans: utiliser électrodes pédiatriques si disponibles"
        - "Enfant < 1 an: DSA contre-indiqué, RCP manuelle uniquement"
        - "Femme enceinte: pas de contre-indication, procéder normalement"
      applicableTriageCategories:
        - "ROUGE"
  
